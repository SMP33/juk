rostopic pub /JUK/TARGET juk_msg/juk_set_target_data_msg "{fly_mode: 4,system: 2, break_distance_mode: 1, data_x: 0.0, data_y: 0.0, data_z: 1.5, speed: 1.0, course: 0.0, acc: 0.2}"
# rostopic pub /JUK/TARGET juk_msg/juk_set_target_data_msg "{fly_mode: 1,system: 2, break_distance_mode: 1, data_x: 0.0, data_y: 0.0, data_z: 2.0, speed: 3.0, course: 0.0, acc: 0.4}" &
 
for (( i=20; i >= 0; i-- ))
do
echo "Landing after $i"
sleep 1
done
echo "LANDING!!!"
rostopic pub /JUK/TARGET juk_msg/juk_set_target_data_msg "{fly_mode: 4,system: 2, break_distance_mode: 1, data_x: 0.0, data_y: 0.0, data_z: 1.5, speed: 1.0, course: 0.0, acc: 0.2}"